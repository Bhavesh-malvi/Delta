{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "uploads/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "methods": ["OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "https://deltawaresolution.com",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept, X-Requested-With",
                "Access-Control-Max-Age": "86400"
            },
            "status": 204,
            "continue": true
        },
        {
            "src": "/api/(.*)",
            "dest": "/index.js",
            "headers": {
                "Access-Control-Allow-Origin": "https://deltawaresolution.com"
            }
        },
        {
            "src": "/uploads/(.*)",
            "dest": "/uploads/$1",
            "headers": {
                "Access-Control-Allow-Origin": "https://deltawaresolution.com",
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}